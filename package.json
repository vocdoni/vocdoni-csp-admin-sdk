{
  "name": "vocdoni-admin-sdk",
  "packageManager": "yarn@3.3.0",
  "author": "Vocdoni",
  "version": "0.0.1",
  "description": "⚒️An SDK for building applications on top of Vocdoni Admin API",
  "repository": "https://github.com/vocdoni/vocdoni-admin-sdk.git",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./umd": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.umd.js"
    },
    "./package.json": "./package.json"
  },


  "scripts": {
    "clean": "rimraf node_modules dist",
    "start": "yarn watch",
    "build": "rollup -c",
    "ts-types": "tsc --emitDeclarationOnly --outDir dist",
    "test": "jest test",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:api": "jest test/api",
    "lint": "eslint src test --ext .js,.jsx,.ts,.tsx",
    "lintfix": "yarn lint --fix",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "watch": "yarn build --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "512 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "512 KB"
    },
    {
      "path": "dist/index.umd.js",
      "limit": "512 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "esbuild": "^0.17.18",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.6",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@size-limit/file": "^8.2.4",
    "axios": "^1.4.0",
    "blakejs": "^1.2.1"
  }
}
